package com.kh.first; // 패키지 선언부

import java.util.Scanner;

public class Main { 
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		String str = sc.nextLine();
		String  [] arr = str.split(" ");
		String a = arr[0];
		String b = arr[1];
		String c = arr[2];
		System.out.println(a+b+c);
		
	}
}
