package com.kh.first; // 패키지 선언부

import java.math.BigInteger;
import java.util.Scanner;

public class Main {
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt()/4;
		
		for(int i=0;i<a;i++) {
			
			
			System.out.print("long ");
		}
		System.out.print("int");
	}
}
