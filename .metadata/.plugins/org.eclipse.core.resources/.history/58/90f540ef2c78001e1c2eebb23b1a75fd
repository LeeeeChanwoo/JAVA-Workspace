package com.kh.first; // 패키지 선언부

import java.math.BigInteger;
import java.util.Scanner;

public class Main {
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int b = sc.nextInt();
	
	
		if((a>=11 && a<=16) && b==0) {
			System.out.println(320);
			
		} else {
			System.out.println(280);
		}


		}
}