package com.kh.first; // 패키지 선언부


import java.util.Scanner;

public class Main { 
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		  int j = sc.nextInt();
		  sc.nextLine();
		  for(int i=0;i<j;i++) {
			  String  S = sc.nextLine();
			  
			  System.out.println(S.charAt(0)+S.charAt(S.length()-1));
		  }
		
		
	
}
}
