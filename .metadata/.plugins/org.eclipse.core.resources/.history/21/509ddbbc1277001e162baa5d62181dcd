package com.kh.first; // 패키지 선언부

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		sc.nextLine();
		 int[] arr = new int[N];	
		


		for (int i = 0; i < N; i++) {
			arr[i] = sc.nextInt();	
		}
		int V = sc.nextInt();
		
		int a = 0;
		for (int j = 0; j < arr.length; j++) {
			if (V==arr[j]) {
				a += 1;
			}
		}
		System.out.println(a);

	}
}
